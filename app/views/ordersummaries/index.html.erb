<div class="page-header">
  <h1>Ordersummaries</h1>
</div>

<p><%= link_to 'Add a New Ordersummary', new_ordersummary_url, class: 'btn btn-primary' %></p>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Order date</th>
      <th>Total bill</th>
      <th>Student</th>
      <th>Order status</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @ordersummaries.each do |ordersummary| %>
    <tr>
        <td><%= ordersummary.order_date %></td>
        <td><%= ordersummary.total_bill %></td>
        <td><%= ordersummary.student_id %></td>
        <td><%= ordersummary.order_status %></td>
  
      <td><%= link_to '<i class="icon-zoom-in"></i>'.html_safe, ordersummary_url(ordersummary), class: 'btn btn-primary' %></td>
      <td><%= link_to '<i class="icon-edit"></i>'.html_safe, edit_ordersummary_url(ordersummary), class: 'btn btn-warning' %></td>
      <td><%= link_to '<i class="icon-trash"></i>'.html_safe, ordersummary_url(ordersummary), method: 'delete', data: { confirm: "Do you really want to delete this ordersummary?" }, class: 'btn btn-danger' %></td>

    </tr>
  <% end %>
  </tbody>
</table>

